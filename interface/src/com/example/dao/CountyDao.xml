<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.dao.CountyDao">
	<!--根据id查询客户信息 -->
	<select id="findCounty" parameterType="Integer"
		                               resultType="County">
		select * from countyinfo where cityID = #{id}
	</select>
</mapper>